<odoo>
    <data>

        <record id="view_hr_entry_type" model="ir.ui.view">
            <field name="name">hr.entry_type.tree</field>
            <field name="model">hr.entry.type</field>
            <field name="arch" type="xml">
                <tree string="Entry Types" editable="top">
                    <field name="name" />
                    <field name="factor" />
                    <field name="is_extra_time" />
                </tree>
            </field>
        </record>

        <record model="ir.actions.act_window" id="hr_entry_type_window">
            <field name="name">Entry Types</field>
            <field name="res_model">hr.entry.type</field>
            <field name="view_mode">tree</field>
            <field name="view_id" ref="view_hr_entry_type" />
        </record>

        <menuitem
      id="hr_entry_type_menuitem"
      name="Entry Types"
      parent="hr_attendance.menu_hr_attendance_manage_attendances"
      sequence="30"
      action="hr_entry_type_window"
    />

    </data>


</odoo>
