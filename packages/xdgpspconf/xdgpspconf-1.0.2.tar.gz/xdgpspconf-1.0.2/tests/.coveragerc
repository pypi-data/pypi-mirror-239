# -*- coding: utf-8; mode: conf; -*-

[report]
exclude_lines =
    pragma: no cover
    if __name__ == '__main__':
    def __repr__
    @nb\.vectorize
    @nb\.njit
    @nb\.jit
ignore_errors = True

[run]
omit =
    */xdgpspconf/__main__.py
    */xdgpspconf/__about__.py
    */xdgpspconf/command_line.py
    */xdgpspconf/errors.py
    */xdgpspconf/native_parallel.py
source = xdgpspconf

[html]
show_contexts = True
